{

  "manifest_version": 2,
  "name": "IMGet - Image Getter",
  "version": "1.0",

  "description": "Gets an image and opens it in a new tab. Right click and select 'IMGet'",
  "homepage_url": "http://adambernste.in",
  "icons": {
    "48": "icons/imget-48.jpg"
  },

  "permissions": [
    "contextMenus",
	"activeTab",
	"tabs"
  ],

  "background": {
    "scripts": ["background.js"]
  }

}